    android:width="48dp"
    android:height="48dp"
    android:viewportHeight="24"
    android:viewportWidth="24"

    app:vc_viewportHeight="24"
    app:vc_viewportWidth="24">
    <group
        android:name="sun">
        <path
            android:name="sun_path"
            android:fillColor="#ffff8000"
            android:pathData="M 6 12 a 3,3 0 0,2 10,0 a 3,3 0 0,2 -10,0"

            app:vc_fillColor="#ffff8000"
            app:vc_pathData="M 6 12 a 3,3 0 0,2 10,0 a 3,3 0 0,2 -10,0" />
        <group
            android:name="earth"
            android:pivotX="12"
            android:pivotY="12">
            <path
                android:name="earth_path"
                android:fillColor="#ff5656ea"
                android:pathData="m 0 12 a 2,2 0 0,2 4,0 a 2,2 0 0,4 -4,0"

                app:vc_fillColor="#ff5656ea"
                app:vc_pathData="m 0 12 a 2,2 0 0,2 4,0 a 2,2 0 0,4 -4,0" />
        </group>
    </group>
</vector>